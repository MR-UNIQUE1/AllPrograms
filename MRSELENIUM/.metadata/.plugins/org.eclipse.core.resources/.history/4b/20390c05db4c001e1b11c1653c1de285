package general.utility.programs;

import java.util.Scanner;

public class Prime_Number {

	public static void main(String[] args) {
		int count =0;
		Scanner s = new Scanner(System.in);
		System.out.println("enter a number");
		int num= s.nextInt();
		for (int i = 1; i <=num; i++) {
			if (num%i==0) {
				count++;
			}
			
		}
	}

}
